function ReclockAudio(clip c)
{
	AR = c.AudioRate()
	TR = Float(AR)*AR*c.FrameCount()/(c.AudioLengthF()*c.FrameRate())
	KScale = Int($7FFFFFFF/TR)
	c.ResampleAudio(Round(TR*KScale), KScale)
	c.AssumeSampleRate(AR)
}

